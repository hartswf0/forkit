{
    "corpus_id": "fork_tracks_entities_v1",
    "corpus_title": "THE FORK: Entities on Tracks — Second-Order Paradox Flows",
    "schema": {
        "description": "Each scenario positions ENTITIES on numbered tracks facing a DECIDER at the switch. The decider chooses which track(s) to route entities through. Branching consequences flow from track selection.",
        "structure": {
            "track": "A numbered channel carrying entities toward outcomes",
            "entity": "An actor whose fate depends on track routing",
            "decider": "The agent at the switch point who selects tracks",
            "switch_point": "The moment of irrevocable routing",
            "terminal": "The outcome station at the end of each track"
        },
        "operators": {
            "ROUTE": "Direct entity from Track A to Track B",
            "MERGE": "Combine multiple tracks into one",
            "SPLIT": "Divide one track into multiple",
            "BLOCK": "Prevent entity from proceeding",
            "DELAY": "Hold entity before routing"
        }
    },
